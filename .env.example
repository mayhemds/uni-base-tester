# Database Configuration Examples

# PostgreSQL
DATABASE_URL=postgresql://username:password@localhost:5432/database
DB_TYPE=postgresql
DB_HOST=localhost
DB_PORT=5432
DB_NAME=myapp
DB_USER=username
DB_PASSWORD=password
DB_SSL=false

# Supabase
NEXT_PUBLIC_SUPABASE_URL=https://xxxxx.supabase.co
